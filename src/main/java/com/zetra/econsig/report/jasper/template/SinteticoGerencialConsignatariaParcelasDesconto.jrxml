	<parameter name="RESPONSAVEL" class="com.zetra.econsig.helper.seguranca.AcessoSistema"/>
	<query language="sql"><![CDATA[SELECT prd.prd_data_desconto as nome, 
count(*) as quantidade, 
CASE WHEN prd.prd_vlr_previsto <> prd_vlr_realizado AND prd.prd_vlr_realizado > 0 THEN 'Desconto Parcial' ELSE spd.spd_descricao END as status 
FROM tb_parcela_desconto prd
INNER JOIN tb_status_parcela_desconto spd ON (prd.spd_codigo = prd.spd_codigo) 
INNER JOIN tb_aut_desconto ade ON (ade.ade_codigo = prd.ade_codigo) 
INNER JOIN tb_verba_convenio vco ON (vco.vco_codigo = ade.vco_codigo) 
INNER JOIN tb_convenio cnv ON (cnv.cnv_codigo = vco.cnv_codigo) 
INNER JOIN tb_consignataria csa ON (csa.csa_codigo = cnv.csa_codigo) 
WHERE prd.prd_data_desconto BETWEEN date_add(current_date(), interval  -36 month) AND current_date() 
GROUP BY prd_data_desconto, spd.spd_codigo]]></query>
