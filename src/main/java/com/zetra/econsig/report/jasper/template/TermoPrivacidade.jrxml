	<query language="sql"><![CDATA[select usu_nome,
usu_login, usu_email,
usu_tel,
usu_cpf,
stu_descricao,
case
when cse.cse_codigo is not null then cse.cse_nome
when org.org_codigo is not null then org.org_nome
when csa.csa_codigo is not null then csa.csa_nome
when cor.cor_codigo is not null then cor.cor_nome
when ser.ser_codigo is not null then 'Servidor'
when supcse.cse_codigo is not null then 'Suporte'
end AS ENTIDADE,
ous.ous_data,
ous.ous_ip_acesso,
case
when ous.toc_codigo in (111,163) then 'WEB'
end AS ACEITACAO_VIA
from tb_ocorrencia_usuario ous
inner join tb_usuario usu on (ous.usu_codigo = usu.usu_codigo)
inner join tb_status_login stu on (usu.stu_codigo = stu.stu_codigo)
left join tb_usuario_cor ucor on (ucor.usu_codigo = usu.usu_codigo)
left join tb_correspondente cor on (cor.cor_codigo = ucor.cor_codigo)
left join tb_usuario_csa ucsa on (ucsa.usu_codigo = usu.usu_codigo)
left join tb_consignataria csa on (csa.csa_codigo = ucsa.csa_codigo)
left join tb_usuario_cse ucse on (ucse.usu_codigo = usu.usu_codigo)
left join tb_consignante cse on (cse.cse_codigo = ucse.cse_codigo)
left join tb_usuario_org uorg on (uorg.usu_codigo = usu.usu_codigo)
left join tb_orgao org on (org.org_codigo = uorg.org_codigo)
left join tb_usuario_ser user on (user.usu_codigo = usu.usu_codigo)
left join tb_servidor ser on (ser.ser_codigo = user.ser_codigo)
left join tb_usuario_sup usup on (usup.usu_codigo = usu.usu_codigo)
left join tb_consignante supcse on (supcse.cse_codigo = usup.cse_codigo)
where 1=1
and ous.toc_codigo in (111,163);]]></query>
