	<query language="sql"><![CDATA[select rrs.rrs_codigo AS RRS_CODIGO,
tmr.tmr_codigo AS TMR_CODIGO, tmr.tmr_descricao AS TMR_DESCRICAO,
rse.rse_matricula AS RSE_MATRICULA,
concat(concat(rse.rse_matricula, ' - '),ser.ser_nome) AS SERVIDOR,
ser.ser_nome AS SER_NOME, ser.ser_cpf AS SER_CPF,
concat(concat(csa.csa_identificador, ' - '), case when trim(coalesce(csa.csa_nome_abrev, '')) = '' then csa.csa_nome else csa.csa_nome_abrev end) AS CONSIGNATARIA,
csa.csa_codigo AS CSA_CODIGO, csa.csa_identificador AS CSA_IDENTIFICADOR, csa.csa_nome AS CSA_NOME,
rrs.rrs_data AS RRS_DATA, rrs.rrs_texto AS RRS_TEXTO, rrs.rrs_ip_acesso AS RRS_IP_ACESSO
from tb_reclamacao_registro_ser rrs
inner join tb_registro_servidor rse on (rrs.rse_codigo = rse.rse_codigo)
inner join tb_servidor ser on (rse.ser_codigo = ser.ser_codigo)
inner join tb_consignataria csa on (csa.csa_codigo = rrs.csa_codigo)
inner join tb_reclamacao_motivo reclamacaoMotivo on (reclamacaoMotivo.rrs_codigo = rrs.rrs_codigo)
inner join tb_tipo_motivo_reclamacao tmr on (tmr.tmr_codigo = reclamacaoMotivo.tmr_codigo)
order by csa.csa_codigo, rrs.rrs_codigo, tmr.tmr_codigo, rrs.rrs_data]]></query>
