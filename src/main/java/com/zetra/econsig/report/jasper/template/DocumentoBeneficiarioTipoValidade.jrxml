	<query language="sql"><![CDATA[select
	tb_registro_servidor.rse_matricula,
	tb_servidor.ser_nome,
	tb_beneficiario.bfc_cpf,
	tb_beneficiario.bfc_nome,
	tb_beneficiario.bfc_telefone,
	tb_tipo_arquivo.tar_descricao,
	tb_anexo_beneficiario.abf_data_validade
from
	tb_aut_desconto
left join tb_tipo_lancamento on tb_tipo_lancamento.TLA_CODIGO = tb_aut_desconto.TLA_CODIGO
left join tb_tipo_natureza on tb_tipo_lancamento.TNT_CODIGO = tb_tipo_natureza.tnt_codigo
left join tb_registro_servidor on tb_aut_desconto.rse_codigo = tb_registro_servidor.rse_codigo
left join tb_contrato_beneficio on tb_aut_desconto.CBE_CODIGO = tb_contrato_beneficio.CBE_CODIGO
left join tb_beneficiario on tb_contrato_beneficio.BFC_CODIGO = tb_beneficiario.BFC_CODIGO
left join tb_servidor on tb_beneficiario.SER_CODIGO = tb_servidor.ser_codigo
left join tb_anexo_beneficiario on tb_beneficiario.BFC_CODIGO = tb_anexo_beneficiario.BFC_CODIGO
left join tb_tipo_arquivo on tb_anexo_beneficiario.TAR_CODIGO = tb_tipo_arquivo.TAR_CODIGO]]></query>
